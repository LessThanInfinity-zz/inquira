<h1>Listing answer_upvotes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_upvotes.each do |answer_upvote| %>
      <tr>
        <td><%= link_to 'Show', answer_upvote %></td>
        <td><%= link_to 'Edit', edit_answer_upvote_path(answer_upvote) %></td>
        <td><%= link_to 'Destroy', answer_upvote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer upvote', new_answer_upvote_path %>
